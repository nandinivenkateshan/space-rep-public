
{
  "presets": ["@babel/preset-react"],
  "env": {
    "development": {
      "presets": [["@babel/preset-react", { "development": true }]],
      "plugins": [["transform-es2015-modules-commonjs"],  ["@babel/plugin-transform-runtime",
        {
          "regenerator": true
        }
      ]]
    },
    "production": {
      "presets": ["@babel/preset-react"],
      "plugins": [["transform-es2015-modules-commonjs"],  ["@babel/plugin-transform-runtime",
        {
          "regenerator": true
        }
      ]]
    }
  }
}