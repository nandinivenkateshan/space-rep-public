    "test": "echo \"Error: no test specified\" && exit 1",
    "server": "nodemon --exec  node-env-run bin/www.js ",
    "dev": "run-p server start"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:3000",
